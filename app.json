{
    "expo": {
        "name": "Gamebook",
        "slug": "Gamebook",
        "version": "1.0.2",
        "orientation": "portrait",
        "icon": "./assets/icon-gb.png",
        "splash": {
            "image": "./assets/splash-gb.png",
            "backgroundColor": "#000000"
        },
        "userInterfaceStyle": "light",
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.felipe.test",
            "userInterfaceStyle": "light"
        },
        "android": {
            "userInterfaceStyle": "light",
            "package": "com.dutra.gamebook",
            "adaptiveIcon": {
                "foregroundImage": "./assets/icon-gb.png",
                "backgroundColor": "#FFFFFF"
            },
            "versionCode": 5
        },
        "plugins": [
            "@react-native-google-signin/google-signin"
        ],
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "extra": {
            "eas": {
                "projectId": "79be229a-6116-4842-9143-0aedb477b724"
            }
        }
    }
}